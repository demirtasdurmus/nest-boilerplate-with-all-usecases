{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "paths": {
      "@app/dynamic-test": [
        "libs/dynamic-test/src"
      ],
      "@app/dynamic-test/*": [
        "libs/dynamic-test/src/*"
      ],
      "@app/lazy-test": [
        "libs/lazy-test/src"
      ],
      "@app/lazy-test/*": [
        "libs/lazy-test/src/*"
      ],
      "@app/logger": [
        "libs/logger/src"
      ],
      "@app/logger/*": [
        "libs/logger/src/*"
      ],
      "@app/bcrypt": [
        "libs/bcrypt/src"
      ],
      "@app/bcrypt/*": [
        "libs/bcrypt/src/*"
      ]
    }
  }
}